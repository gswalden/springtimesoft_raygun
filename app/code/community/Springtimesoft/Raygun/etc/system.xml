<?xml version="1.0" encoding="UTF-8"?>
<config>
    <tabs>
        <springtimesoft_raygun translate="label" module="springtimesoft_raygun">
            <label>Springtimesoft</label>
            <sort_order>1000</sort_order>
        </springtimesoft_raygun>
    </tabs>
    <sections>
        <springtimesoft_raygun translate="label" module="springtimesoft_raygun">
            <label>Raygun</label>
            <tab>springtimesoft_raygun</tab>
            <sort_order>1000</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <springtimesoft_raygun_group translate="label" module="springtimesoft_raygun">
                    <label>General Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <springtimesoft_raygun_logo translate="label">
                            <frontend_type>label</frontend_type>
                            <label><![CDATA[
                            <div style="width:600px">
                                <a href="https://raygun.io" target="_blank"><img src="
data:image/png;base64," width="150" alt="Raygun" title="Raygun" /></a>
                            </div>
                            ]]></label>
                            <sort_order>0</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </springtimesoft_raygun_logo>
                        <springtimesoft_raygun_enabled translate="label">
                            <label>Enable Error Reporting</label>
                            <comment>Whether or not to allow errors to be reported by the extension.</comment>
                            <frontend_type>select</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </springtimesoft_raygun_enabled>
                        <springtimesoft_raygun_api_key translate="label">
                            <label>Api Key</label>
                            <comment><![CDATA[Your application API key. You can find this by opening <a href="https://app.raygun.io/dashboard/" target="_blank">https://app.raygun.io/dashboard/</a> and navigating to <strong>Application Settings -> Settings</strong>.]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </springtimesoft_raygun_api_key>
                        <springtimesoft_raygun_instructions translate="label">
                            <frontend_type>label</frontend_type>
                            <label><![CDATA[
<div style="width:600px;margin-top:60px">
<p>To catch exceptions this code snippet must also be inserted into <strong><code>app/Mage.php</code></strong> around line <strong><code>693</code></strong>.</p>
<p><em>Without these lines in place only regular errors will be caught. Please note that this is a core hack, and care should be taken when upgrading Magento in the future.</em></p>
<pre style="padding-left:10px;background:#111;color:#fff;font-size:11px"><code>
    die();
} catch (Exception $e) {
    // BEGIN
    if (class_exists('Springtimesoft_Raygun_Helper_Data')) {
        self::helper('springtimesoft_raygun')->exceptionHandler($e);
    }
    // END

    if (self::isInstalled() || self::$_isDownloader) {

</code></pre>
</div>
                            ]]></label>
                            <sort_order>100</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </springtimesoft_raygun_instructions>
                    </fields>
                </springtimesoft_raygun_group>
                <springtimesoft_raygun_advanced_group translate="label" module="springtimesoft_raygun">
                    <label>Advanced Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>2000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <springtimesoft_raygun_async translate="label">
                            <label>Use Asynchronous Error Sending</label>
                            <comment><![CDATA[When sending error messages, control whether messages are sent in the background (this may not work in some environments). Requires <strong><code>curl</code></strong> to be installed.]]></comment>
                            <frontend_type>select</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </springtimesoft_raygun_async>
                        <springtimesoft_raygun_tags translate="label">
                            <label>Tags</label>
                            <comment><![CDATA[Extra tags to add to reported errors, separate tags with a space (e.g. <code>magento php</code>).]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </springtimesoft_raygun_tags>
                    </fields>
                </springtimesoft_raygun_advanced_group>
            </groups>
        </springtimesoft_raygun>
    </sections>
</config>
